{
    "root": true,
    "env": {
        "browser": false,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
     "plugins": [
         "react"
    ],
    "rules": {
        "linebreak-style": 0,
        "import/extensions": ["error", "always", {
            "js": "never",
            "jsx": "never",
            "mjs": "never",
            "ts": "never",
            "tsx": "never"
          }]
    }
}
